    "import re\n",
    "\n",
    "texto = \"Entre em contato em exemplo@email.com ou suporte@outroemail.com.\"\n",
    "padrao = r'\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b'\n",
    "emails = re.findall(padrao, texto)\n",
    "print(emails)"
   ]
